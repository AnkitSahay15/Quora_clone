/* Main Primary color */
#loginForm {
  width: 400px;
  margin: 0 auto;
  padding: 10px;
  background-color: white;
  -webkit-box-shadow: 0px 14px 23px 1px rgba(112, 112, 112, 0.86);
  -moz-box-shadow: 0px 14px 23px 1px rgba(112, 112, 112, 0.86);
  box-shadow: 0px 14px 23px 1px rgba(112, 112, 112, 0.86);
  margin-top: 50px; }

#authentication {
  height: 100%;
  width: 100%; }

.btn.btn-social {
  margin-right: 7px;
  min-width: 100px; }

.btn.btn-facebook {
  color: white;
  background-color: #3B5998;
  border-color: #37528C; }

.btn.btn-facebook:hover,
.btn.btn-facebook:focus {
  color: white;
  background-color: #2F487B;
  border-color: #2F487B; }

.btn.btn-google {
  color: white;
  background-color: #dc4e41;
  border-color: #C1453A; }

.btn.btn-google:hover,
.btn.btn-google:focus {
  color: white;
  background-color: #C74539;
  border-color: #AF4138; }

.title {
  color: #B92B27;
  text-align: center; }

.buttons {
  text-align: center; }
  .buttons .quora-submit {
    background-color: #B92B27; }
  .buttons .quora-change {
    background-color: #B92B27;
    border-bottom: 4px solid #c8c8c8;
    border-right: 1px solid #c8c8c8;
    border-left: 1px solid #c8c8c8;
    outline: none; }
  .buttons .quora-change:hover {
    border-bottom: 0px solid #c8c8c8;
    border-right: 0px solid #c8c8c8;
    border-left: 0px solid #c8c8c8; }
  .buttons .quora-change:active {
    border-bottom: 0px solid #c8c8c8;
    border-right: 0px solid #c8c8c8;
    border-left: 0px solid #c8c8c8;
    outline: none; }
  .buttons .animated {
    -webkit-transition: all .1s ease;
    -moz-transition: all .1s ease;
    -ms-transition: all .1s ease;
    -o-transition: all .1s ease;
    transition: all .1s ease; }
  .buttons .animated:active {
    -webkit-transition: all 0s ease;
    -moz-transition: all 0s ease;
    -ms-transition: all 0s ease;
    -o-transition: all 0s ease;
    transition: all 0s ease; }

